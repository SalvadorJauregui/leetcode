/* Solution 1*/
class Solution {
    public int singleNumber(int[] nums) {
        int x; = nums[0];
        for (int i = 1; i < nums.length; i++)
            /* XOR operation holds the single number */
            x ^= nums[i];
        return x;
    }
}

/* Solution 2*/
class Solution {
    public int singleNumber(int[] nums) {
        int x=0;
        for(int n : nums)
            x ^= n;
        return x;
    }
}

/* Solution 1 */
class Solution {
    public int majorityElement(int[] nums) {
        int elem = 0, count = 0;
        for (int i : nums){
            if (count == 0){
                elem = i;
                count = 1;
            }else if (i == elem){
                ++count;
            }else{
                --count;
            }
        }
        return elem;
    }
}

/* Solution 2 */
class Solution {
    public int majorityElement(int[] nums) {
        Arrays.sort(nums);
        return nums[nums.length/2];
    }
}
